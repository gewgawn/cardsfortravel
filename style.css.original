.dropdown-menu > li > a:hover,
.dropdown-menu > li > a:focus,
.dropdown-submenu:hover > a,
.dropdown-submenu:focus > a {
  text-decoration: none;
  color: #ffffff;
  background-color: ##00877c;
  background-image: -moz-linear-gradient(top, #00877c, #00877c);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#00877c), to(#00877c));
  background-image: -webkit-linear-gradient(top, #00877c, #00877c);
  background-image: -o-linear-gradient(top, #00877c, #00877c);
  background-image: linear-gradient(to bottom, #00877c, #00877c);
  background-repeat: repeat-x;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00877c', endColorstr='#00877c', GradientType=0);
}
.dropdown-menu > .active > a,
.dropdown-menu > .active > a:hover,
.dropdown-menu > .active > a:focus {
  color: #ffffff;
  text-decoration: none;
  outline: 0;
  background-color: #00877c;
  background-image: -moz-linear-gradient(top, #00877c, #00877c);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#00877c), to(#00877c));
  background-image: -webkit-linear-gradient(top, #00877c, #00877c);
